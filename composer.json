{
    "name": "stillat/relationships",
    "type": "statamic-addon",
    "description": "Provides bi-directional entry relationship automation",
    "keywords": [
        "statamic",
        "entries"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "John Koster",
            "email": "john@stillat.com",
            "homepage": "https://stillat.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "statamic/cms": "^3.3.21"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "orchestra/testbench": "^6.7.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Stillat\\Relationships\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Relationships",
            "description": "Relationships addon"
        },
        "laravel": {
            "providers": [
                "Stillat\\Relationships\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
